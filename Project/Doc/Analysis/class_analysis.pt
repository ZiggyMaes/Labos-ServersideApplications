@startuml

title Class analysis

User "1" -- "*" Message
Rating "*" -- "*" Message
User "1" -- "*" Rating

class User {
    userid: int
    e-mail: string
    password-hash: string
    date-registered: datetime
    last-login: datetime
    superuser: bool
    admin: bool
    blocked: bool
    inactive: bool

    +block() / unblock()
    +promote() / demote()
}

class Message {
    messageid: int
    title: string
    body: string
    time-posted: datetime
    thread: bool
    visible: bool
    areaid: int
    userid: int
    
    +create()/delete()
    +rate() / derate()
    +edit()
    +show() / hide()
    +search()
}

class Area{
    title: String
    image: IMage
    enzovoort
}

Message "*" -- "1" Area
User "*" -- "2" Area

class Rating {
    ratingid: int
    stars: int
    messageid: int
    userid: int
}

class Statistics {
    [static] generateStats()
}



@enduml